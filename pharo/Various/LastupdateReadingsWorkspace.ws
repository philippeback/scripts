files := CXFileSystemService default rrdLastupdateFiles.file := files first.lines :=  file contents splitOn: Character lf.fields := (lines at: 1) splitOn: Character space.values := (lines at: 2) splitOn: Character space.valueOfIn := values at: (fields indexOf: 'in').valueOfOut := values at: (fields indexOf: 'out').timestamp := DateAndTime fromUnixTime: ((values at: 1) allButLast) asInteger